{
  "projectType": "library",
  "sourceRoot": "libs/shared/ui/ui-modal-dialog/src",
  "prefix": "snarlabs",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/shared/ui/ui-modal-dialog"],
      "options": {
        "project": "libs/shared/ui/ui-modal-dialog/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/ui/ui-modal-dialog/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/ui/ui-modal-dialog/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/ui/ui-modal-dialog"],
      "options": {
        "jestConfig": "libs/shared/ui/ui-modal-dialog/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/ui/ui-modal-dialog/**/*.ts",
          "libs/shared/ui/ui-modal-dialog/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:shared", "type:ui", "context:shared:ui:modal-dialog"]
}
